{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "no-unused-vars": "off",
        "no-multiple-empty-lines": "off",
        "no-trailing-spaces": "off",
        "indent": "off",
        "padded-blocks": "off",
    }
}
